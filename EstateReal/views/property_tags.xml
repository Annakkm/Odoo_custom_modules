<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="views_tags_form" model="ir.ui.view">
        <field name="name">estate.property.tags.form</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
