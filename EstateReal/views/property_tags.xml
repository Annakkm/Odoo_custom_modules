    <record id="views_tags_tree" model="ir.ui.view">
        <field name="name">estate.property.tags.tree</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>


</odoo>